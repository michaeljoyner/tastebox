        "less-loader": "^6.0.0",
        "prettier": "^2.0.5",
        "sortablejs": "^1.10.2",
        "tailwindcss": "^1.4.6",
        "vue": "^2.6.11",
        "vue-router": "^3.1.6",
        "vuex": "^3.3.0"
    }
